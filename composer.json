{
  "name": "hemilio/leetcode",
  "description": "leet code resolution codes",
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.30",
    "phpunit/phpunit": "^10.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Hemilio Melo",
      "email": "hemilioaraujo@gmail.com"
    }
  ],
  "scripts": {
    "phpunit": [
      "php vendor/bin/phpunit"
    ],
    "testColor": [
      "@phpunit --color"
    ],
    "testWarning": [
      "@testColor --display-warnings"
    ],
    "test": [
      "@testWarning ./tests"
    ]
  }
}
